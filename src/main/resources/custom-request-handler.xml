<requestHandler name="/gbsearch" class="solr.SearchHandler">

    <lst name="defaults">
        <int name="sort">namespace_id desc</int>
        <int name="rows">10</int>
    </lst>

    <lst name="appends">
        <str name="fq">NOT sysid1:""</str>
    </lst>

    <lst name="invariants">
        <str name="wt">json</str>
    </lst>

</requestHandler>